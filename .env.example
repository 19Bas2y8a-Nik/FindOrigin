# Telegram Bot Token (получить у @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Опциональный секрет для безопасности webhook
TELEGRAM_WEBHOOK_SECRET=

# API ключ для поискового сервиса
# Варианты: Google Custom Search, SerpAPI, Bing Search
SEARCH_API_KEY=your_search_api_key_here

# Выбор поискового движка: google, serpapi, bing
SEARCH_ENGINE=google

# ID поисковой системы Google (только для Google Custom Search)
GOOGLE_SEARCH_ENGINE_ID=your_google_cse_id_here

# AI: OpenRouter с моделью openai/gpt-4o-mini (формирование запроса и ранжирование)
OPENROUTER_API_KEY=

# Устаревший ключ (не используется при наличии OPENROUTER_API_KEY)
AI_API_KEY=
